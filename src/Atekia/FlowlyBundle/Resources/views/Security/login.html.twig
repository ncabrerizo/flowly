{% extends 'AtekiaFlowlyBundle:Layout:default.html.twig' %}

{% block title %}
    {{ 'login.title'|trans }}
{% endblock %}

{% block content %}

    <div class="row">
        <div class="col-sm-8 col-md-6 col-lg-4 col-sm-offset-2 col-md-offset-3 col-lg-offset-4">
            <div class="account-wall">
                {# <img class="profile-img" src="data:image/png;base64," alt=""--> #}
                <img class="profile-img" src="data:image/png;base64," alt="">
                <form class="form-signin" action="{{ path('login_check') }}" method="post">
                    {% if error %}
                        <div class="alert alert-danger">
                            {{ 'login.error'|trans }}
                        </div>
                    {% endif %}
                    <input type="text" class="form-control" placeholder="{{ 'login.username'|trans }}" required autofocus name="_username" value="{{ last_username }}">
                    <input type="password" class="form-control" placeholder="{{ 'login.password'|trans }}" required name="_password">
                    <button class="btn btn-lg btn-primary btn-block" type="submit">
                        {{ 'login.signin'|trans }}
                    </button>
                    <div class="checkbox pull-left">
                        <label>
                            <input type="checkbox" name="_remember_me">
                            {{ 'login.rememberme'|trans }}
                        </label>
                    </div>
                    <a href="#" class="bootbox-alert pull-right need-help" data-message="{{ 'login.helpmessage'|trans }}">{{ 'login.needhelp'|trans }} </a>
                    <span class="clearfix"></span>
                </form>
            </div>
        </div>
    </div>

{% endblock %}

{% block javascripts %}{% endblock %}